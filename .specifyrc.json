{
  "repository": "@frip/FDS",
  "head": "specifyrc-json",
  "base": "main",
  "version": "2",
  "rules": [
    {
      "name": "SDTF",
      "parsers": [
        {
          "name": "to-tailwind",
          "output": {
            "type": "file",
            "filePath": "web/theme.js"
          }
        },
        {
          "name": "to-swift",
          "output": {
            "type": "file",
            "filePath": "ios/tokens.swift"
          }
        },
        {
          "name": "to-kotlin",
          "output": {
            "type": "file",
            "filePath": "android/tokens.kt"
          }
        }
      ]
    }
  ]
}
